import java.util.Scanner;
class NumberPattern_12
{
	public static void main(String[] args)
	{
		int n,i,j;
		Scanner input=new Scanner(System.in);
		System.out.println("enter the n values:");
		n=input.nextInt();
		for(i=1;i<=2*n-1;i++)
		{
			for(j=i;j<i+n;j++)
			{
				System.out.print(j);
			}
			System.out.println();
		}
	}
}